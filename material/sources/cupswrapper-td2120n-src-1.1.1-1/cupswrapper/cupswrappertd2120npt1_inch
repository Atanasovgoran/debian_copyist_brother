if [ "$1" = '-e' ]; then
# comments
if [ -f /etc/debian_version ] ; then
sudo rm -rf /var/lib/dpkg/info/td2120ncupswrapper*
fi
# comments
#*********** Below line uncommented on 20120830_1130am **********
  lpadmin -x TD-2120N
  rm -f /usr/share/cups/model/brother_td2120n_printer_en.ppd
#*********** Below line commented  on 20120830_1130am **********
#  rm -f /usr/lib/cups/backend/brusb_td2120n
#  rm -f /usr/lib/cups/backend/brserial_td2120n
#*********** Below line commented  on 20120830_1130am **********
#  rm -f /usr/lib64/cups/backend/brusb_td2120n
#  rm -f /usr/lib64/cups/backend/brserial_td2120n
  rm -f /usr/lib64/cups/filter/
  rm -f /opt/brother/PTouch/td2120n/cupswrapper/brcupsconfpt1
if [  -e /etc/init.d/cups ]; then
   /etc/init.d/cups restart
elif [  -e /etc/init.d/cupsys ]; then
   /etc/init.d/cupsys restart
fi
#  /etc/init.d/cups restart
  exit 0
fi
if [ "$1" = "-r" ]; then
#*********** Below line uncommented  on 20120830_1130am **********
  lpadmin -x TD-2120N
if [  -e /etc/init.d/cups ]; then
   /etc/init.d/cups restart
elif [  -e /etc/init.d/cupsys ]; then
   /etc/init.d/cupsys restart
fi
#  /etc/init.d/cups restart
  exit 0
fi
if [ "$1" = "help" ] || [ "$1" = "-h" ]; then
  echo   'option -h : help'
  echo   '       -i : install'
  echo   '       -e : uninstall'
  echo   '       -r : remove printer'
  exit 0
fi
#mkdir -p /opt/brother/PTouch/ql720nw/filter
#mkdir -p /usr/lib/cups/filter

if [ -e "/opt/brother/PTouch/td2120n/lpd/filtertd2120n" ]; then
  :
else
  echo "ERROR : Brother LPD filter is not installed."
fi
#comments
if [ -f /etc/debian_version ] ; then 
if [  -d /usr/share/cups/model ]; then
rm -f /usr/share/cups/model/brother_td2120n_printer_en.ppd
ppd_file_name=/usr/share/cups/model/brother_td2120n_printer_en.ppd
ppd_file_name1=/usr/share/cups/model/brother_td2120n_printer_en.ppd
fi
if [  -d /usr/share/ppd ]; then
rm -f /usr/share/ppd/brother_td2120n_printer_en.ppd
ppd_file_name1=/usr/share/ppd/brother_td2120n_printer_en.ppd
fi
else 
rm -f /usr/share/cups/model/brother_td2120n_printer_en.ppd
ppd_file_name=/usr/share/cups/model/brother_td2120n_printer_en.ppd
ppd_file_name1=/usr/share/cups/model/brother_td2120n_printer_en.ppd
