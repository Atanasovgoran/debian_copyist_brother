brotherlpdwrapper=/usr/lib/cups/filter/brother_lpdwrapper_td2120n
brotherlpdwrapper64=/usr/lib64/cups/filter/brother_lpdwrapper_td2120n
#brotherlpdwrapper=/usr/lib/cups/backend/brusb_td2120n
#brotherlpdwrapper64=/usr/lib64/cups/backend/brusb_td2120n

rm -f  $brotherlpdwrapper
#echo 'rm -f /usr/lib/cups/filter/brlpdwrappertd2120n'


cat <<!ENDOFWFILTER! >$brotherlpdwrapper
#! /bin/sh
#
# Brother Print filter  >>  /usr/lib/cups/backend/brusb_td2120n
# Copyright (C) 2006-2013 Brother. Industries, Ltd.
#                                    Ver1.10

# This program is free software; you can redistribute it and/or modify it
# under the terms of the GNU General Public License as published by the Free
# Software Foundation; either version 2 of the License, or (at your option)
# any later version.
#
# This program is distributed in the hope that it will be useful, but WITHOUT
# ANY WARRANTY; without even the implied warranty of MERCHANTABILITY or
# FITNESS FOR A PARTICULAR PURPOSE.  See the GNU General Public License for
# more details.
#
# You should have received a copy of the GNU General Public License along with
# this program; if not, write to the Free Software Foundation, Inc., 59 Temple
# Place, Suite 330, Boston, MA  02111-1307  USA
#

LOGFILE="/dev/null"
LOGLEVEL="1"
LOGCLEVEL="7"
DEBUG=0
NUPENABLE=1
LOG_LATESTONLY=1

touch /tmp/td2120n_latest_print_info
chmod 600 -R /tmp/td2120n_latest_print_info

errorcode=0

if [ \$DEBUG != 0 ]; then
    LOGFILE=/tmp/br_cupsfilter_debug_log
fi

PPDC=\`printenv | grep "PPD="\`
PPDC=\`echo \$PPDC | sed -e 's/PPD=//'\`

if [ "\$PPDC" = "" ]; then
    PPDC="/usr/share/cups/model/brother_td2120n_printer_en.ppd"
fi


if [ \$LOGFILE != "/dev/null" ]; then
  if [ \$LOG_LATESTONLY == "1" ]; then
    rm -f \$LOGFILE
    date                                                           >\$LOGFILE
  else
    if [ -e \$LOGFILE ]; then
	    date                                                        >>\$LOGFILE
    else
	    date                                                        >\$LOGFILE
    fi
  fi
    echo "arg0 = \$0"                                           >>\$LOGFILE
    echo "arg1 = \$1"                                           >>\$LOGFILE
    echo "arg2 = \$2"                                           >>\$LOGFILE
    echo "arg3 = \$3"                                           >>\$LOGFILE
    echo "arg4 = \$4"                                           >>\$LOGFILE
    echo "arg5 = \$5"                                           >>\$LOGFILE
    echo "arg6 = \$6"                                           >>\$LOGFILE
    echo "PPD  = \$PPD"                                         >>\$LOGFILE
fi

cp  /opt/brother/PTouch/td2120n/inf/brtd2120nrc  /tmp/brtd2120nrc_\$$
chmod 777 -R /tmp/brtd2120nrc_\$$
export BRPRINTERRCFILE=/tmp/brtd2120nrc_\$$

INPUT_TEMP_PS=\`mktemp /tmp/br_input_ps.XXXXXX\`

nup="cat"
if [ "\`echo \$5 | grep 'Nup='\`" != '' ] && [ \$NUPENABLE != 0 ]; then

	if   [ "\`echo \$5 | grep 'Nup=64'\`" != '' ]; then
		nup="psnup -64"
	elif [ "\`echo \$5 | grep 'Nup=32'\`" != '' ]; then
		nup="psnup -32"
	elif [ "\`echo \$5 | grep 'Nup=25'\`" != '' ]; then
		nup="psnup -25"
	elif [ "\`echo \$5 | grep 'Nup=16'\`" != '' ]; then
		nup="psnup -16"
	elif [ "\`echo \$5 | grep 'Nup=8'\`" != '' ]; then
		nup="psnup -8"
	elif [ "\`echo \$5 | grep 'Nup=6'\`" != '' ]; then
		nup="psnup -6"
	elif [ "\`echo \$5 | grep 'Nup=4'\`" != '' ]; then
		nup="psnup -4"
	elif [ "\`echo \$5 | grep 'Nup=2'\`" != '' ]; then
		nup="psnup -2"
	elif [ "\`echo \$5 | grep 'Nup=1'\`" != '' ]; then
		nup="cat"
	fi
	echo   "NUP=\$nup"                                      >>\$LOGFILE
   if [ -e /usr/bin/psnup ]; then
       if [ \$# -ge 7 ]; then
	       cat \$6  | \$nup > \$INPUT_TEMP_PS
       else
	       cat       | \$nup > \$INPUT_TEMP_PS
       fi
   else
       if [ \$# -ge 7 ]; then
	       cp \$6  \$INPUT_TEMP_PS
       else
	       cat    > \$INPUT_TEMP_PS
       fi
   fi
else
   if [ \$# -ge 7 ]; then
      cp \$6  \$INPUT_TEMP_PS
   else
      cat    > \$INPUT_TEMP_PS
   fi
fi
if [ -e "/opt/brother/PTouch/td2120n/lpd/filtertd2120n" ]; then
       :
else
    echo "ERROR: /opt/brother/PTouch/td2120n/lpd/filtertd2120n does not exist"   >>\$LOGFILE
    echo "ERROR: /opt/brother/PTouch/td2120n/lpd/filtertd2120n does not exist"   >> /tmp/td2120n_latest_print_info
    errorcode=30
    exit
fi

CUPSOPTION=\`echo "\$5 Copies=\$4" | sed -e 's/PageSize/media/' -e 's/BrMargin/Feed/' -e 's/BrCompress/Compress/' -e 's/BrCutAtEnd/CutAtEnd/' -e  's/BrTrimtape/Trimtape/' -e 's/BrRotate/RotatePrint/' -e 's/BrPeeler/Peeler/' -e 's/BrCutLabel/CutLabel/' -e 's/BrMirror/MirrorPrint/' -e 's/BrPriority/Priority/' -e 's/BrQuality/QUALITY/' -e 's/BrSpeed/SPEED/' -e 's/BrHalftonePattern/Halftone/' -e 's/BrErrorDiffusion/ERROR/' -e 's/BrBinary/BINARY/' -e 's/BrDither/DITHER/' -e 's/BrBrightness/Brightness/' -e 's/BrContrast/Contrast/' -e 's/multiple-document-handling/Collate/' -e 's/separate-documents-collated-copies/ON/' -e 's/separate-documents-uncollated-copies/OFF/'\`




BRURI=\`printenv | grep DEVICE_URI=\`
BRURI=\`echo \$BRURI | grep "brserial"\`
PORT="USB"
if [ "\$BRURI" != "" ]; then
	PORT="SERIAL"
fi

if [ -e "/opt/brother/PTouch/td2120n/cupswrapper/brcupsconfpt1" ]; then
  if [ \$DEBUG = 0 ]; then
     /opt/brother/PTouch/td2120n/cupswrapper/brcupsconfpt1  td2120n  \$PPDC 0 "\$CUPSOPTION" \$BRPRINTERRCFILE>> /dev/null
  else
     /opt/brother/PTouch/td2120n/cupswrapper/brcupsconfpt1  td2120n  \$PPDC \$LOGCLEVEL "\$CUPSOPTION" \$BRPRINTERRCFILE>>\$LOGFILE
  fi
fi

if [ \$DEBUG -lt 10 ]; then
    cat    \$INPUT_TEMP_PS | /opt/brother/PTouch/td2120n/lpd/filtertd2120n "\$\$" "CUPS" "\$PORT"
    echo brtd2120nrc_\$$   > /tmp/td2120n_latest_print_info
    cat  /tmp/brtd2120nrc_\$$  >> /tmp/td2120n_latest_print_info
    rm -f /tmp/brtd2120nrc_\$$
    if [ \$LOGLEVEL -gt 2 ];  then
	   if [ \$LOGFILE != "/dev/null" ]; then
	     echo ""                                                >>\$LOGFILE
	     echo "    ------PostScript Data-------"                >>\$LOGFILE
	     cat    \$INPUT_TEMP_PS                                  >>\$LOGFILE
	   fi
    fi
fi
rm -f  \$INPUT_TEMP_PS

exit $errorcode

!ENDOFWFILTER!

chmod a+x $brotherlpdwrapper
if [ -e /usr/lib64/cups/backend ]; then
   cp $brotherlpdwrapper  $brotherlpdwrapper64
fi

#chmod a+w /opt/brother/PTouch/td2120n/inf/brtd2120nrc
#chmod a+w /opt/brother/PTouch/td2120n/inf
if [ -e /etc/init.d/lpd ]; then
   /etc/init.d/lpd stop
fi
if [  -e /etc/init.d/lprng ]; then
   /etc/init.d/lprng stop
fi

#*********** Below line commented  on 20120830_1130am **********
#cp -f /usr/local/Brother/PTouch/td2120n/lpd/brusb_ql_lpr /usr/lib/cups/backend/brusb_td2120n
#cp -f /usr/local/Brother/PTouch/td2120n/lpd/brserial_td2120n /usr/lib/cups/backend/brserial_td2120n
#chmod 00777 /usr/lib/cups/backend/brusb_td2120n
#chmod 00777 /usr/lib/cups/backend/brserial_td2120n

if [  -e /etc/init.d/cups ]; then
   /etc/init.d/cups restart
elif [  -e /etc/init.d/cupsys ]; then
   /etc/init.d/cupsys restart
fi

sleep 2s

#*********** Below line commented  on 20120830_1130am **********
#lpadmin -p TD-2120N -E -v brusb_qtd2120n:/dev/usb -m brtd2120n.ppd
#lpadmin -p TD-2120N -E -v usb://Brother/TD-2120N -P /usr/share/ppd/brtd2120n.ppd

port2=`lpinfo -v | grep -i 'usb://Brother/TD-2120N' | head -1`
if [ "$port2" = '' ];then
    port2=`lpinfo -v | grep 'usb://Brother' | head -1`
fi

if [ "$port2" = '' ];then
    port2=`lpinfo -v | grep 'usb://' | head -1`
fi
port=`echo $port2| sed s/direct//g`

if [ "$port" = '' ];then
    port=usb:/dev/usb/lp0
fi

lpadmin -p TD-2120N -E -v $port -P /usr/share/cups/model/brother_td2120n_printer_en.ppd

exit 0

