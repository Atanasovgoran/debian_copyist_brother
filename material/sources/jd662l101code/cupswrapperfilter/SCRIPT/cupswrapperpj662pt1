brotherlpdwrapper=/usr/lib/cups/filter/brlpdwrapperpj662
brotherlpdwrapper64=/usr/lib64/cups/filter/brlpdwrapperpj662
#brotherlpdwrapper=/usr/lib/cups/backend/brusb_pj662
#brotherlpdwrapper64=/usr/lib64/cups/backend/brusb_pj662

rm -f  $brotherlpdwrapper
#echo 'rm -f /usr/lib/cups/filter/brlpdwrapperpj662'


cat <<!ENDOFWFILTER! >$brotherlpdwrapper
#! /bin/sh
#
# Brother Print filter  >>  /usr/lib/cups/backend/brusb_pj662
# Copyright (C) 2006 Brother. Industries, Ltd.
#                                    Ver1.10

# This program is free software; you can redistribute it and/or modify it
# under the terms of the GNU General Public License as published by the Free
# Software Foundation; either version 2 of the License, or (at your option)
# any later version.
#
# This program is distributed in the hope that it will be useful, but WITHOUT
# ANY WARRANTY; without even the implied warranty of MERCHANTABILITY or
# FITNESS FOR A PARTICULAR PURPOSE.  See the GNU General Public License for
# more details.
#
# You should have received a copy of the GNU General Public License along with
# this program; if not, write to the Free Software Foundation, Inc., 59 Temple
# Place, Suite 330, Boston, MA  02111-1307  USA
#

LOGFILE="/dev/null"
LOGLEVEL="1"
LOGCLEVEL="7"
DEBUG=0
NUPENABLE=1
LOG_LATESTONLY=1
errorcode=0

if [ \$DEBUG != 0 ]; then
    LOGFILE=/tmp/br_cupsfilter_debug_log
fi

PPDC=\`printenv | grep "PPD="\`
PPDC=\`echo \$PPDC | sed -e 's/PPD=//'\`

if [ "\$PPDC" = "" ]; then
if [ -d /usr/share/cups/model ]; then
    PPDC="/usr/share/cups/model/brpj662.ppd"
elif [ -d /usr/share/ppd ]; then
PPDC="/usr/share/ppd/brpj662.ppd"
fi
fi


if [ \$LOGFILE != "/dev/null" ]; then
  if [ \$LOG_LATESTONLY == "1" ]; then
    rm -f \$LOGFILE
    date                                                           >\$LOGFILE
  else
    if [ -e \$LOGFILE ]; then
	    date                                                        >>\$LOGFILE
    else
	    date                                                        >\$LOGFILE
    fi
  fi
    echo "arg0 = \$0"                                           >>\$LOGFILE
    echo "arg1 = \$1"                                           >>\$LOGFILE
    echo "arg2 = \$2"                                           >>\$LOGFILE
    echo "arg3 = \$3"                                           >>\$LOGFILE
    echo "arg4 = \$4"                                           >>\$LOGFILE
    echo "arg5 = \$5"                                           >>\$LOGFILE
    echo "arg6 = \$6"                                           >>\$LOGFILE
    echo "PPD  = \$PPD"                                         >>\$LOGFILE
fi

INPUT_TEMP_PS=\`mktemp /tmp/br_input_ps.XXXXXX\`

nup="cat"
if [ "`echo \$5 | grep 'Nup='`" != '' ] && [ \$NUPENABLE != 0 ]; then

	if   [ "`echo \$5 | grep 'Nup=64'`" != '' ]; then
		nup="psnup -64"
	elif [ "`echo \$5 | grep 'Nup=32'`" != '' ]; then
		nup="psnup -32"
	elif [ "`echo \$5 | grep 'Nup=25'`" != '' ]; then
		nup="psnup -25"
	elif [ "`echo \$5 | grep 'Nup=16'`" != '' ]; then
		nup="psnup -16"
	elif [ "`echo \$5 | grep 'Nup=8'`" != '' ]; then
		nup="psnup -8"
	elif [ "`echo \$5 | grep 'Nup=6'`" != '' ]; then
		nup="psnup -6"
	elif [ "`echo \$5 | grep 'Nup=4'`" != '' ]; then
		nup="psnup -4"
	elif [ "`echo \$5 | grep 'Nup=2'`" != '' ]; then
		nup="psnup -2"
	elif [ "`echo \$5 | grep 'Nup=1'`" != '' ]; then
		nup="cat"
	fi
	echo   "NUP=\$nup"                                      >>\$LOGFILE
   if [ -e /usr/bin/psnup ]; then
       if [ \$# -ge 7 ]; then
	       cat \$6  | \$nup > \$INPUT_TEMP_PS
       else
	       cat       | \$nup > \$INPUT_TEMP_PS
       fi
   else
       if [ \$# -ge 7 ]; then
	       cp \$6  \$INPUT_TEMP_PS
       else
	       cat    > \$INPUT_TEMP_PS
       fi
   fi
else
   if [ \$# -ge 7 ]; then
      cp \$6  \$INPUT_TEMP_PS
   else
      cat    > \$INPUT_TEMP_PS
   fi
fi
if [ -e "/usr/local/Brother/PTouch/pj662/lpd/filterpj662" ]; then
       :
else
    echo "ERROR: /usr/local/Brother/PTouch/pj662/lpd/filterpj662 does not exist"   >>\$LOGFILE
    errorcode=30
    exit
fi

CUPSOPTION=\`echo "\$5 Copies=\$4" | sed -e 's/BrCompress/Compress/' -e 's/BrCutAtEnd/CutAtEnd/' -e 's/BrTrimtape/Trimtape/' -e 's/BrCutLabel/CutLabel/' -e 's/PageSize/media/' -e 's/BrMargin/Feed/' -e 's/BrAutoTapeCut/AutoCut/' -e 's/BrMirror/MirrorPrint/' -e 's/BrPriority/Priority/' -e 's/BrQuality/QUALITY/' -e 's/BrSpeed/SPEED/' -e 's/BrHalftonePattern/Halftone/' -e 's/BrDensity/Density/' -e 's/Br2PlyMode/2PlyMode/' -e 's/BrNegativePrint/NegativePrint/'-e 's/BrErrorDiffusion/ERROR/' -e 's/BrBinary/BINARY/' -e 's/BrDither/DITHER/' -e 's/BrFormFeedMode/formfeed/' -e 's/BrFixed/FIXED/' -e 's/BrEndofpage/ENDOFPAGE/' -e 's/BrNofeed/NOFEED/' -e 's/BrEndofpageretract/ENDOFPAGERETRACT/' -e 's/BrExtraFeedMode/extrafeed/' -e 's/BrNone/NONE/' -e 's/BrHalfinch/HALF/' -e 's/BrOneinch/ONE/' -e 's/BrOneandhalfinch/ONEANDHALF/' -e 's/BrTwoinch/TWO/' -e 's/BrBrightness/Brightness/' -e 's/multiple-document-handling/Collate/' -e 's/separate-documents-collated-copies/ON/' -e 's/separate-documents-uncollated-copies/OFF/'\`
BRURI=\`printenv | grep DEVICE_URI=\`
BRURI=\`echo \$BRURI | grep "brserial"\`
PORT="USB"
if [ "\$BRURI" != "" ]; then
	PORT="SERIAL"
fi
if [ -e "/usr/local/Brother/PTouch/pj662/cupswrapper/brcupsconfpt1" ]; then
  if [ \$DEBUG = 0 ]; then
     /usr/local/Brother/PTouch/pj662/cupswrapper/brcupsconfpt1  pj662  \$PPDC 0 "\$CUPSOPTION" >> /dev/null
  else
     /usr/local/Brother/PTouch/pj662/cupswrapper/brcupsconfpt1  pj662  \$PPDC \$LOGCLEVEL "\$CUPSOPTION" >>\$LOGFILE
  fi
fi

if [ \$DEBUG -lt 10 ]; then
    cat    \$INPUT_TEMP_PS | /usr/local/Brother/PTouch/pj662/lpd/filterpj662 "\$\$" "CUPS" "\$PORT"

    if [ \$LOGLEVEL -gt 2 ];  then
	   if [ \$LOGFILE != "/dev/null" ]; then
	     echo ""                                                >>\$LOGFILE
	     echo "    ------PostScript Data-------"                >>\$LOGFILE
	     cat    \$INPUT_TEMP_PS                                  >>\$LOGFILE
	   fi
    fi
fi
rm -f  \$INPUT_TEMP_PS

exit $errorcode

!ENDOFWFILTER!

chmod a+x $brotherlpdwrapper
if [ -e /usr/lib64/cups/backend ]; then
   cp $brotherlpdwrapper  $brotherlpdwrapper64
fi

chmod a+w /usr/local/Brother/PTouch/pj662/inf/brpj662rc
chmod a+w /usr/local/Brother/PTouch/pj662/inf
if [ -e /etc/init.d/lpd ]; then
   /etc/init.d/lpd stop
fi
if [  -e /etc/init.d/lprng ]; then
   /etc/init.d/lprng stop
fi

cp -f /usr/local/Brother/PTouch/pj662/lpd/brusb_pj_lpr /usr/lib/cups/backend/brusb_pj662
#cp -f /usr/local/Brother/PTouch/pj662/lpd/brserial_pj662 /usr/lib/cups/backend/brserial_pj662
#chmod 00777 /usr/lib/cups/backend/brusb_pj662
#chmod 00777 /usr/lib/cups/backend/brserial_pj662

if [  -e /etc/init.d/cups ]; then
   /etc/init.d/cups restart
elif [  -e /etc/init.d/cupsys ]; then
   /etc/init.d/cupsys restart
fi

sleep 2s

#lpadmin -p PJ-662 -E -v USB:/dev/usb -m brpj662.ppd
lpadmin -p PJ-662 -E -v usb:/dev/usb/lp0 -m brpj662.ppd

exit 0

