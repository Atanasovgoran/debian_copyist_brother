if [ "$1" = '-e' ]; then

if [ -f /etc/debian_version ] ; then
sudo rm -rf /var/lib/dpkg/info/pj662cupswrapper*
fi

  lpadmin -x PJ-662
  rm -f /usr/share/cups/model/brpj662.ppd
  rm -f /usr/lib/cups/backend/brusb_pj662
  rm -f /usr/lib/cups/backend/brserial_pj662
  rm -f /usr/lib64/cups/backend/brusb_pj662
  rm -f /usr/lib64/cups/backend/brserial_pj662
  rm -f /usr/lib64/cups/filter/
  rm -f /usr/local/Brother/PTouch/pj662/cupswrapper/brcupsconfpt1
if [  -e /etc/init.d/cups ]; then
   /etc/init.d/cups restart
elif [  -e /etc/init.d/cupsys ]; then
   /etc/init.d/cupsys restart
fi
#  /etc/init.d/cups restart
  exit 0
fi
#S3
if [ "$1" = "-r" ]; then
  lpadmin -x PJ-662
if [  -e /etc/init.d/cups ]; then
   /etc/init.d/cups restart
elif [  -e /etc/init.d/cupsys ]; then
   /etc/init.d/cupsys restart
fi
#  /etc/init.d/cups restart
  exit 0
fi
if [ "$1" = "help" ] || [ "$1" = "-h" ]; then
  echo   'option -h : help'
  echo   '       -i : install'
  echo   '       -e : uninstall'
  echo   '       -r : remove printer'
  exit 0
fi
#mkdir -p /usr/local/Brother/PTouch/pj662/filter
#mkdir -p /usr/lib/cups/filter

if [ -e "/usr/local/Brother/PTouch/pj662/lpd/filterpj662" ]; then
  :
else
  echo "ERROR : Brother LPD filter is not installed."
fi

if [ -f /etc/debian_version ] ; then 
if [  -d /usr/share/cups/model ]; then
rm -f /usr/share/cups/model/brpj662.ppd
ppd_file_name=/usr/share/cups/model/brpj662.ppd
fi
if [  -d /usr/share/ppd ]; then
rm -f /usr/share/ppd/brpj662.ppd
ppd_file_name1=/usr/share/ppd/brpj662.ppd
fi
else 
rm -f /usr/share/cups/model/brpj662.ppd
ppd_file_name=/usr/share/cups/model/brpj662.ppd
ppd_file_name1=/usr/share/cups/model/brpj662.ppd
fi

cat <<ENDOFPPDFILE | tee $ppd_file_name >> $ppd_file_name1
